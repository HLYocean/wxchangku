/* pages/channel/channel.wxss */
.list {
    position: relative;
    display: flex;
    float: left;
    width: 50vw;
    height: 50vw;
    justify-content: center;
    align-items: center;
}

image {
    display: block;
    width: 50vw;
    height: 100%;
}

text {
    position: absolute;
    padding-bottom: 10px;
    z-index: 9;
    color: #fff;
    border-bottom: 4px solid #fff;
}

